{
  "compilerOptions": {
    "baseUrl": ".",
    "allowSyntheticDefaultImports": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "jsx": "react",
    "module": "ESNext",
    "target": "ES6",
    //"target": "esnext",
    "lib": ["DOM", "ES5", "ES6", "ES7", "ESNext"],
    //"lib": ["dom", "esnext"],
    "allowJs": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "skipLibCheck": true,
    /* Linting */
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "isolatedModules": true,
    "noEmit": true
    //
  },
  "include": ["src"]
}
